<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="root">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="IncludeLocalFileSystem" type="xs:string" minOccurs="0" />
        <xs:element name="InlcudeAzureStorage" type="xs:string" minOccurs="0" />
        <xs:element name="IncludeAzureBlobStorage" type="xs:string" minOccurs="0" />
        <xs:element name="IncludeAzureQueueStorage" type="xs:string" minOccurs="0" />
        <xs:element name="IncludeAzureTableStorage" type="xs:string" minOccurs="0" />
        <xs:element name="OverwriteBlobIfExists" type="xs:string" minOccurs="0" />
        <xs:element name="IncludeNetworkFileSytesm" type="xs:string" minOccurs="0" />
        <xs:element name="AzureAccountName" type="xs:string" minOccurs="0" />
        <xs:element name="AzureAccountSharedKey" type="xs:string" minOccurs="0" />
        <xs:element name="AzureBlobEndPoint" type="xs:string" minOccurs="0" />
        <xs:element name="AzureQueueEndPoint" type="xs:string" minOccurs="0" />
        <xs:element name="AzureTableEndPoint" type="xs:string" minOccurs="0" />
        <xs:element name="DefaultContainerName" type="xs:string" minOccurs="0" />
        <xs:element name="DefaultBlockSizeInMB" type="xs:string" minOccurs="0" />
        <xs:element name="AllowDeletionOfContainers" type="xs:string" minOccurs="0" />
        <xs:element name="AllowDeletionOfBlobs" type="xs:string" minOccurs="0" />
        <xs:element name="AllowDeletionOfQueue" type="xs:string" minOccurs="0" />
        <xs:element name="AllowDeletionOfMessages" type="xs:string" minOccurs="0" />
        <xs:element name="AllowDeletionOfTables" type="xs:string" minOccurs="0" />
        <xs:element name="AllowDeletionOfTableEntities" type="xs:string" minOccurs="0" />
        <xs:element name="DisplayViewPanel" type="xs:string" minOccurs="0" />
        <xs:element name="DisplayHiddenFilesAndFolders" type="xs:string" minOccurs="0" />
        <xs:element name="ToolTipTimeoutInSeconds" type="xs:string" minOccurs="0" />
        <xs:element name="NetworkLocations" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Location" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="root" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>